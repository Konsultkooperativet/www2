wget -qO- \
  "https://github.com/yihui/tinytex/raw/master/tools/install-unx.sh" | sh

JEKYLL_ENV=production jekyll build

~/bin/tlmgr install latex-base
~/bin/tlmgr install enumitem
~/bin/tlmgr install ifmtarg
~/bin/tlmgr install geometry
~/bin/tlmgr install fancyhdr
~/bin/tlmgr install xcolor
~/bin/tlmgr install ifxetex
~/bin/tlmgr install xifthen
~/bin/tlmgr install etoolbox
~/bin/tlmgr install setspace
~/bin/tlmgr install fontspec
~/bin/tlmgr install unicode-math
~/bin/tlmgr install fontawesome
~/bin/tlmgr install sourcesanspro
~/bin/tlmgr install tcolorbox
~/bin/tlmgr install parskip
~/bin/tlmgr install hyperref

cp /opt/build/repo/_site/*.tex _latex/
ls -l /opt/build/repo/_latex/
cd  /opt/build/repo/_latex/
~/bin/xelatex markusberglund

