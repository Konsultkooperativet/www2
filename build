wget -qO- \
  "https://github.com/yihui/tinytex/raw/master/tools/install-unx.sh" | sh

JEKYLL_ENV=production jekyll build

~/bin/tlmgr install latex-base enumitem ifmtarg fix-cm fancyhdr xcolor xifthen setspace unicode-math fontawesome sourcesanspro tcolorbox parskip ragged2e fontawesome roboto pgf environ

cp /opt/build/repo/_site/*.tex _latex/

cd  /opt/build/repo/_latex/
ls /opt/build/repo/_latex/fonts/

~/bin/xelatex markusberglund

